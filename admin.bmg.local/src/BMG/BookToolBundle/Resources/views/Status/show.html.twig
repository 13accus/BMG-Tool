{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Status</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Statusname</th>
                <td>{{ entity.statusName }}</td>
            </tr>
            <tr>
                <th>Statusid</th>
                <td>{{ entity.statusId }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('status') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('status_edit', { 'id': entity.statusId }) }}">
            Edit
        </a>
    </li>
</ul>
{% endblock %}
