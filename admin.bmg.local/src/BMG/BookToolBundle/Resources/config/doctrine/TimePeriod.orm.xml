<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="BMG\BookToolBundle\Entity\TimePeriod" table="time_period">
    <indexes>
      <index name="fk_time_period_status1_idx" columns="status_id"/>
    </indexes>
    <id name="timePeriodId" type="integer" column="time_period_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="timePeriodName" type="string" column="time_period_name" length="45" nullable="true"/>
    <many-to-one field="status" target-entity="Status">
      <join-columns>
        <join-column name="status_id" referenced-column-name="status_id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
